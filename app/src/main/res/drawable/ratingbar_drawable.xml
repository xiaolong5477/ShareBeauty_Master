    <item
        android:id="@android:id/background"
        android:drawable="@drawable/star_large_empty">
    </item>
    <item
        android:id="@android:id/secondaryProgress"
        android:drawable="@drawable/star_large_empty">
    </item>
    <item
        android:id="@android:id/progress"
        android:drawable="@drawable/star_large">
    </item>

</layer-list>